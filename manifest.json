{
    "manifest_version": 3,
    "name": "Pokemon Central Wiki Redirect",
    "version": "1.0",
    "description": "Redirects Pokemon names to Pokemon Central Wiki",
    "permissions": ["webNavigation", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon-16.png",
        "48": "icon-48.png"
      }
    },
    "manifest_key": {
      "public": "<YOUR_PUBLIC_KEY>"
    },
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  }
  